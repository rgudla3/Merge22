<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#CEF09D">


    <!-- test -->
    <View
        android:id="@+id/plasticBottleTree"
        android:layout_width="match_parent"
        android:layout_height="705dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp"
        android:background="@drawable/checklist_bg" />

    <View
        android:id="@+id/plasticBottleRect"
        android:layout_width="211dp"
        android:layout_height="59dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="156dp"
        android:layout_marginTop="47dp"
        android:background="#00000000" />

    <View
        android:id="@+id/plasticBottleRectangleWhite"
        android:layout_width="211dp"
        android:layout_height="59dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="153dp"
        android:layout_marginTop="89dp"
        android:background="@drawable/rectangle_4" />

    <TextView
        android:id="@+id/plasticBottleHeader"
        android:layout_width="179dp"
        android:layout_height="43dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="170dp"
        android:layout_marginTop="96dp"
        android:fontFamily="@font/patrick"
        android:gravity="center_horizontal|top"
        android:text="Paper"
        android:textAppearance="@style/hello_usern" />

    <TextView
        android:id="@+id/answerQuestions"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="-1dp"
        android:layout_marginTop="165dp"
        android:fontFamily="@font/patrick"
        android:gravity="center_horizontal|top"
        android:text="Please answer the following questions to proceed:"
        android:textAppearance="@style/pick_item_t" />


    <View
        android:layout_width="275dp"
        android:layout_height="68dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="75dp"
        android:layout_marginTop="277dp"
        android:background="@drawable/rectangle_4" />

    <TextView
        android:layout_width="275dp"
        android:layout_height="55dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="87dp"
        android:layout_marginTop="295dp"
        android:fontFamily="@font/patrick"
        android:gravity="center_horizontal|top"
        android:text="Is your paper soiled or wet?"
        android:textSize="22sp" />

    <Button
        android:id="@+id/frostedNoButton"
        android:layout_width="155dp"
        android:layout_height="52dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="216dp"
        android:layout_marginTop="364dp"
        android:background="#00000000" />

    <View
        android:id="@+id/foodWasteNoUnselected"
        android:layout_width="142dp"
        android:layout_height="48dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="209dp"
        android:layout_marginTop="368dp"
        android:background="@drawable/rectangle_8" />

    <Button
        android:id="@+id/foodWasteYesButton"
        android:layout_width="155dp"
        android:layout_height="52dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="47dp"
        android:layout_marginTop="364dp"
        android:background="#00000000" />

    <View
        android:id="@+id/frostedYesUnselected"
        android:layout_width="143dp"
        android:layout_height="48dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="48dp"
        android:layout_marginTop="367dp"
        android:background="@drawable/rectangle_8" />

    <View
        android:id="@+id/frostedYesSelected"
        android:layout_width="144dp"
        android:layout_height="48dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="47dp"
        android:layout_marginTop="367dp"
        android:background="@drawable/rectangle_4"
        android:visibility="invisible" />

    <View
        android:id="@+id/frostedNoSelected"
        android:layout_width="145dp"
        android:layout_height="51dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="209dp"
        android:layout_marginTop="365dp"
        android:background="@drawable/rectangle_4"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/frostedNoText"
        android:layout_width="153dp"
        android:layout_height="48dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="204dp"
        android:layout_marginTop="366dp"
        android:fontFamily="@font/patrick"
        android:gravity="center_horizontal|top"
        android:text="No"
        android:textAppearance="@style/return_name"
        android:textSize="32sp" />

    <TextView
        android:id="@+id/frostedYesText"
        android:layout_width="143dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="49dp"
        android:layout_marginTop="369dp"
        android:fontFamily="@font/patrick"
        android:gravity="center_horizontal|top"
        android:text="Yes"
        android:textAppearance="@style/return_name"
        android:textSize="32sp" />

    <View
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="46dp"
        android:layout_marginTop="282dp"
        android:background="@drawable/small_white_rect" />

    <TextView
        android:id="@+id/questionOne"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="69dp"
        android:layout_marginTop="290dp"
        android:fontFamily="@font/patrick"
        android:gravity="center_horizontal|top"
        android:text="1"
        android:textAppearance="@style/pick_item_t" />

    <Button
        android:id="@+id/continueArrow"
        android:layout_width="wrap_content"
        android:layout_height="84dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="160dp"
        android:layout_marginTop="501dp"
        android:background="#00000000" />

    <View
        android:id="@+id/goArrow"
        android:layout_width="93dp"
        android:layout_height="86dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="159dp"
        android:layout_marginTop="501dp"
        android:background="@drawable/go_arrow" />

</RelativeLayout>